<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
       android:shape="rectangle">
    <corners
        android:radius="50dp"
        />
    <gradient
        android:angle="135"
        android:centerX="35%"
        android:centerColor="#00AA00"
        android:startColor="#003300"
        android:endColor="#00FF00"
        android:type="linear"
        />
    <padding
        android:left="1dp"
        android:top="1dp"
        android:right="1dp"
        android:bottom="1dp"
        />
    <size
        android:width="0dp"
        android:height="0dp"
        />
    <stroke
        android:width="3dp"
        android:color="#878787"
        />
</shape>
